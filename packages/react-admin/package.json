{
  "name": "@promopixel/react-admin",
  "version": "0.0.3",
  "main": "./lib/index.js",
  "files": [
    "lib",
    "dist"
  ],
  "module": "./lib-es/index.js",
  "sideEffects": false,
  "typings": "./lib/index.d.ts",
  "author": "Panter AG",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build:lib",
