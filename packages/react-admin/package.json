    "@mui/icons-material": "^5.8.0",
    "@promopixel/editor": "0.0.3"
  },
  "devDependencies": {
    "react-admin": "^3.12.2",
    "uniforms": "^3.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
